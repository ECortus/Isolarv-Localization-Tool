.c-add-language-button {
    opacity: 1;
    align-items: center;
    align-self: stretch;
    display: flex;
    margin-top: 20px;
    width: 200px;
    height: 22px;
    -unity-background-image-tint-color: rgb(0, 0, 0);
    background-color: rgb(48, 120, 191);
    margin-left: 20px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    top: 20px;
}

.c-add-language-box Button {
    flex-grow: 1;
    max-width: none;
    width: auto;
}

.c-language-list-view {
    top: auto;
    left: auto;
    width: 30%;
    height: 155%;
    align-content: stretch;
    align-items: stretch;
    align-self: stretch;
    border-top-width: 40px;
    justify-content: flex-start;
    flex-basis: auto;
}

.c-main-label {
    -unity-font-style: bold;
    -unity-text-align: lower-left;
    font-size: 16px;
    flex-shrink: 1;
    flex-basis: auto;
    bottom: -30px;
    align-items: auto;
    align-content: auto;
    width: 130px;
    height: 8px;
    left: 15px;
}

.c-content-box {
    background-color: rgb(51, 51, 51);
    width: 70%;
    align-self: stretch;
    height: 350%;
    align-content: auto;
    align-items: stretch;
    justify-content: flex-start;
    flex-direction: column;
    position: absolute;
    flex-basis: auto;
    flex-shrink: 0;
    top: -10px;
    border-right-width: 0;
    left: auto;
}
